    "date-fns": "^2.19.0",
    "date-fns-tz": "^1.1.3",
    "hls.js": "^0.14.17",
    "kaleidoscopejs": "^1.1.1",
    "ky": "^0.27.0",
    "lodash": "^4.17.20",
    "marked": "^2.0.1",
    "mitt": "^2.1.0",
    "plausible-tracker": "^0.3.1",
    "reconnecting-websocket": "^4.4.0",
    "three": "^0.126.1",
    "tone": "^14.7.77",
    "vue": "^3.0.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.11",
    "eslint": "^7.23.0",
    "eslint-plugin-vue": "^7.8.0",
    "vite": "^2.1.5",
    "vite-plugin-fonts": "^0.1.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
